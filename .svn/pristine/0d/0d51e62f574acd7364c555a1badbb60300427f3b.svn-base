
#travelslide {
	position: absolute;
}

.travelads {
	margin: 0;
	height: 310px;
	background: #EFEFEF url(../images/assets/home.jpg) no-repeat;
	position: relative;
	overflow: hidden;
}

#travelslide .travelitem {
	position: absolute;
	width: 980px;
	height: 310px;
	overflow: hidden;
}
                      
#travelslide .travelitem span {
	font-family: "Lucida Grande", "Lucida Sans Unicode", helvetica, verdana, arial, sans-serif;
}

#travelslide .travelitem a {
	text-decoration: none;
}

/* slide 1 */
#travelslide img.img1 {
	position: absolute;
	top: 4px;
	left: 410px;
}

#travelslide img.img2 {
	position: absolute;
	top: 10px;
	left: 400px;
}

#travelslide .travelitem .title {
	position: relative;
	clear: both;
	float: right;
	margin-right: 10px;
	margin-top: 30px;
}

#travelslide .travelitem .price {            
	position: relative;
	clear: both;
	float: right;
	margin-right: 10px;
	margin-top: 0px;
}

#travelslide .travelitem .button {
	position: relative;
	clear: both;
	float: right;
	margin-right: 20px;
	margin-top: 30px;
}

#travelslide .travelitem .text1 {
	position: relative;
	float: left;
	margin: 140px 12px 0 48px;
	color: #333333;
	font-size: 48px;
}

#travelslide .travelitem .text2 {              
	clear: left;
	position: relative;
	float: left;
	margin: 4px 12px 0 48px;
	color: #666666;
	font-size: 24px;
}

