}
.detailstep3 th {
    background: none repeat scroll 0 0 #EEEEEE;
    text-align: left;
}
.detailstep3 td {
    font-weight: normal;
}
.detailstep3 .edit {
    text-align: center;
    width: 100px;
}
.detailstep3 .center {
    text-align: center;
}
.detailstep3 .total {
    color: red;
    font-weight: bold;
}
.detailstep3 .selectpayment table, .detailstep3 .selectpayment td {
    border: medium none;
}
/* End Apply Visa */

/* Extra Service */
.extra-service-banner {
	margin-top: 20px;
}
/* End Extra Service */

/* Pagination */
.pagination {
    margin: 10px 0;
    text-align: center;
}
.pagination a {
    background: -moz-linear-gradient(center top , #F9F9F9 0%, #E8E8E8 100%) repeat scroll 0 0 transparent;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.35);
    color: #717171;
    font: bold 11px Arial,sans-serif;
    margin: 4px;
    padding: 5px 8px;
    text-decoration: none;
    text-shadow: 0 1px white;
}
.pagination a.current {
    background: -moz-linear-gradient(center top , #7CB9E5 0%, #57A1D8 100%) repeat scroll 0 0 transparent;
    box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.8);
    color: white;
    text-shadow: 0 1px #3F789F;
}
.pagination a:hover {
    background: -moz-linear-gradient(center top , #FFFFFF 0%, #E8E8E8 100%) repeat scroll 0 0 transparent;
    box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.55);
}
.pagination a.active, #pagination a.current:active {
    box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.5) inset, 0 1px 1px 0 #FFFFFF !important;
}
.pagination a.current:hover {
    background: -moz-linear-gradient(center top , #99CEFC 0%, #57A1D8 100%) repeat scroll 0 0 transparent;
    box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.9);
}
/* End Pagination */

/* Vietnam Embassies */
#vietnam-embassies {
	margin-top: 20px;
}
.nations {
    margin: 20px 0;
}
.nations .embassies {
    border-top: 1px solid #E9E9E9;
    float: left;
    padding: 15px 0;
