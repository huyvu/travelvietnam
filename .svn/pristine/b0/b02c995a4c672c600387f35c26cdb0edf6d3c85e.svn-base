@CHARSET "ISO-8859-1";

/* Template Layout */

body {
	background: url("../images/background.jpg") no-repeat scroll top center transparent;
	color: #2A2A2A;
}

#home-wizban {
	background: url("../images/wizban-shadow.png") no-repeat scroll center bottom transparent;
	height: 332px;
	position: relative;
}

/* End Template Layout */

/* Custom Controls */

#search-widget {
	background-color: #FFFFFF;
	width: 370px;
	height: 260px;
	margin: 0px;
	padding: 10px;
	position: absolute;
	display: block;
	left: 15px;
	top: 15px;
	z-index: 900;
}

#search-widget .widget-title {
	background: url("../images/title-arrow.png") no-repeat scroll 20px bottom transparent;
	padding-bottom: 5px;
}

#search-widget .widget-title span {
	width: 100%;
	display: block;
	border-bottom: 2px solid #db7803;
	color: #007cc6;
	font-size: 14px;
	font-weight: bold;
	padding-bottom: 5px;
}

#search-widget ul.widget-menu {
	margin-top: 10px;
}

#search-widget ul.widget-menu li {
	float: left;
	width: 68px;
	margin-right: 1px;
	padding: 4px;
	line-height: 18px;
	background-color: #0093ea;
	text-align: center;
	cursor: pointer;
}

#search-widget ul.widget-menu li.selected {
	background-color: #FFF3B0;
}

#search-widget ul.widget-menu li a {
	font-weight: bold;
	text-decoration: none;
	cursor: pointer;
	color: #FFFFFF;
}

#search-widget ul.widget-menu li.selected a {
	color: #0060a3;
}

#search-widget .widget-content {
	clear: both;
	background-color: #FFF3B0;
	height: 175px;
	padding: 10px 0px;
}

#search-widget .widget-content p.text-form {
    display: inline-block;
    float: left;
    margin: 5px 0px 0px 10px;
}

#search-widget .widget-content p.w180 {
	width: 170px;
}

#search-widget .widget-content .text-form label {
	display: block;
	margin-bottom: 4px;
}

#search-widget .widget-content .text-form span {
	background: url("../images/calendar-icon.png") no-repeat scroll 85px 4px #FEFEFE;
	border: 1px solid #AAAAAA;
	float: left;
	padding: 2px 6px 4px 6px;
	width: 90px;
}

#search-widget .widget-content .text-form input.text-input {
	width: 158px;
	border: 1px solid #AAAAAA;
	padding: 2px 6px 4px 6px;
}

#search-widget .widget-content .text-form select {
	width: 172px;
	border: 1px solid #AAAAAA;
	padding: 2px 4px;
}

#search-widget .widget-content .text-form span input.calendar {
	background: none repeat scroll 0 0 transparent;
	border: medium none;
	float: none;
	width: 75px;
	font-size: 12px;
}

#search-widget .widget-content .text-form .map-ticker {
	border: 0px;
	bottom: 22px;
	right: 120px;
	position: absolute;
	cursor: pointer;
}

#search-widget .widget-content .text-form .search-button {
	bottom: 20px;
	right: 15px;
	position: absolute;
	cursor: pointer;
}

#search-widget .widget-content .text-form .findhotel-button {
	background: url("../images/btn-find-hotel.png") no-repeat scroll 0 0 transparent;
	width: 114px;
	height: 41px;
}

#search-widget .widget-content .text-form .findtour-button {
	background: url("../images/btn-find-hotel.png") no-repeat scroll 0 0 transparent;
	width: 114px;
	height: 41px;
}

a.readon, p.readmore a {
    background: url("../images/readmore.png") no-repeat scroll left center transparent;
    color: #E4B490;
    font-size: 100%;
    padding-left: 15px;
}

a.readon:hover, a.readon:focus, a.readon:active, p.readmore a:hover, p.readmore a:focus, p.readmore a:active {
    background-image: url("../images/readmore-orange.png");
    color: #EF761A;
}

.slide-desc {
	float:right;
	margin-top: 220px;
}

.slide-desc a.readon {
    background-image: url("../images/button.png");
    color: #FEFFFF;
    text-shadow: 1px 0 1px rgba(0, 0, 0, 0.3);
    width: 246px;
    height: 36px;
    line-height: 36px;
    display: block;
    text-decoration: none;
    display: inline-block;
    font-size: 115% !important;
    position: absolute;
    right: 10px;
    text-indent: 35px;
}

.slide-desc a.readon:hover {
	opacity: 0.9;
}

/* End Custom Controls */

