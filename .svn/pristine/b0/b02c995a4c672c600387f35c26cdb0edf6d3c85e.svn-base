/* Testimonial */
.fade-slider-container {
    position: relative;
}
.fade-slider {
    position: relative;
    z-index: 5;
}
.fade-slider ul {
    display: block;
    margin: 0;
    overflow: hidden;
    padding: 0;
    width: 100%;
}
.fade-slider li {
    background: none repeat scroll 0 0 transparent;
    display: none;
    float: left;
    padding: 0;
}
.fade-slider li:first-child {
    display: block;
}
.fade-slider img {
    display: block;
    height: auto;
    width: 100%;
}
.motion-effect .slide-overlay {
    display: block;
    height: auto;
    left: 0;
    position: absolute;
    top: 0;
    width: 125%;
}
.main-slider-container .block-background {
    background: none repeat scroll 0 0 #FFFFFF;
    bottom: 0;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    width: 100%;
}
.main-slider {
    border: 10px solid #FFFFFF;
    overflow: hidden;
}
.main-slider .arrow {
    background: url("../images/main-slider-arrows.png") no-repeat scroll -13px 0 transparent;
    cursor: pointer;
    height: 23px;
    margin-top: -12px;
    opacity: 0;
    position: absolute;
    top: 50%;
    width: 13px;
    z-index: 100;
}
.main-slider:hover .arrow {
    opacity: 1;
}
.main-slider .arrow-left {
    left: 24px;
}
.main-slider .arrow-right {
    background-position: 0 0;
    right: 24px;
}
.testimonials-slider {
    margin-top: 40px;
}
.testimonials-slider ul.testimonials li {
    background: none repeat scroll 0 0 #FFFFFF;
}
.testimonials-slider .quote {
    border: 1px solid #E2E2E2;
    font-size: 1.1em;
    font-style: italic;
    line-height: 1.6em;
    padding: 1em 20px;
}
.testimonials-slider .quote-author {
    background: url("../images/testimonials-item-bg.png") no-repeat scroll right 0 transparent;
    color: #444444;
    font-weight: bold;
    float: left;
    margin: -1px 0 0;
    padding: 0.9em 15px 0 20px;
}
.testimonials-slider .controls {
    bottom: 0.4em;
    position: absolute;
    right: 20px;
    z-index: 100;
}
.testimonials-slider .controls a {
    background: url("../images/testimonials-control-bg.png") no-repeat scroll 0 0 #DFDFDF;
    display: block;
    float: left;
    height: 10px;
    margin-left: 5px;
    width: 10px;
}
.testimonials-slider .controls a.current {
    background-color: #FF9000;
}
/* End Testimonial */

