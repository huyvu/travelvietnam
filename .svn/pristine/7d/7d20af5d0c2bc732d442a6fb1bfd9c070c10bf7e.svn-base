}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .view-more {
	margin-top: 10px;
	text-align: right;
}

#vietnam-view-nav {
	width: 280px;
}

#vietnam-view-nav ul {
	margin: 0px;
	padding: 0px;
}

#vietnam-view-nav ul li {
	margin: 0px 0px 5px;
	padding: 0px;
}

#vietnam-view-nav ul li a {
	background-color: #009DAF;
	color: #FFFFFF;
	font-size: 15px;
	font-weight: bold;
	width: 260px;
	padding: 10px;
	display: inline-table;
	text-decoration: none;
