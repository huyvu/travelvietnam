#attraction-reviews .review-content {
	width: 520px;
	padding: 15px 20px 15px 20px;
	background-color: #FFFFFF;
	position: relative;
	border-radius: 10px;
	-moz-border-radius : 10px;
	-webkit-border-radius: 10px;
}

#attraction-reviews > ul li {
	display: block;
	padding-bottom: 20px;
	overflow: hidden;
	border-bottom: 1px solid #cfcfcf;
	margin-top: 20px;
}

#attraction-reviews .review-content:before {
	width: 0px; 
	height: 0px; 
	border-bottom: 5px solid transparent;  /* left arrow slant */
	border-top: 9px solid transparent; /* right arrow slant */
	border-right: 25px solid #ffffff; /* bottom, add background color here */
	font-size: 0;
	line-height: 0;
	position:absolute;
	top:50px;
	left:-20px;
	content:"";
	z-index: 2;
	-webkit-transform: rotate(-15deg);
	-moz-transform: rotate(-15deg);
	-ms-transform: rotate(-15deg);
	-o-transform: rotate(-15deg);
	transform: rotate(-15deg);
}

