/* Template Layout */

#page {
	margin: 0 auto;
	padding: 0;
	width: 100%;
}

#page .page-title {
	line-height: 1.2em;
}

#page .left-img {
}

#page .right-img {
}

#breadcrumbs {
	background: none;
}

#header {
	background-color: #FFFFFF;
}

#banner {
	height: 100px;
}

#pagecontent {
	background-color: transparent;
	padding: 0px;
}

#banner .inner {
	position: relative;
}

.inner {
	margin: 0 auto;
	padding: 0;
	width: 980px;
}

#footer .association {
	background: url("../images/association/bg.jpg") repeat scroll 0 0 transparent;
	padding-top: 10px;
}

#footer .association .association-info {
	background-color: #dff2ff;
	padding: 10px;
}

#footer .association .association-info .text {
	color: #009daf;
	font-weight: bold;
	line-height: 30px;
}

#footer .association .association-info .icons a {
	float: left;
	margin-left: 20px;
}

#footer .info-area {
	background-color: #FFFFFF;
	padding: 25px 10px;
}

#footer .info-area .links {
	float: left;
	margin: 0px;
	margin-right: 10px;
	width: 16%;
}

#footer .info-area h2 {
	color: #2A2A2A;
}

#footer .info-area ul {
	margin-left: 0px;
	margin-top: 10px;
}

#footer .info-area ul li {
	list-style-type: none;
}

#footer .info-area ul li a {
	color: #333333;
}

#footer .company-info {
	background: #FFFFFF;
	border-top: 2px solid #0072bc;
	height: 1em;
	margin-top: 0px;
	padding: 20px 0px 50px;
}

#footer .company-info .copyright {
	padding: 0px;
}

#footer .company-info .copyright div {
	color: #2A2A2A;
}

#footer .company-info .copyright div.left {
	margin-left: 0px;
}

#footer .company-info .copyright div.right {
	margin-right: 0px;
}

#footer .newsletter-signup {
	width: 300px;
}
#footer .newsletter-signup .label {
	color: #333333;
	margin-top: 10px;
}
#footer .newsletter-signup .newsletter-signup-form {
	margin: 4px 0px 20px;
}
#footer .newsletter-signup .newsletter-signup-form .signup-email {
	width: 210px;
	padding: 4px;
}
#footer .newsletter-signup .newsletter-signup-form .signup-button {
	background-color: #008392;
	background-image: -moz-linear-gradient(center top , #008392, #008392);
	background-repeat: repeat-x;
	border: none;
	color: #FFFFFF;
	cursor: pointer;
	display: inline-block;
	font-size: 12px;
	font-weight: bold;
	line-height: normal;
	padding: 5px 10px;
	font-family: Arial, Helvetica, sans-serif;
}
#footer .newsletter-signup .follow-icons {
	margin: 6px 0px 0px;
}
#footer .newsletter-signup .follow-icons a {
	float: left;
	margin-right: 10px;
}

/* End Template Layout */

/* Overview */

.page-title {
	margin-bottom: 10px;
}

#vietnam-view-detailed {
}

#vietnam-view-detailed .overview-name {
	color: #008392;
	font-size: 18px;
	font-weight: normal;
	margin-bottom: 10px;
	padding-bottom: 5px;
}

#vietnam-view-detailed .overview-name .expand-all {
	background: url("../images/tour/icon/expanded.png") no-repeat scroll right center transparent;
	color: #2A2A2A;
	font-weight: normal;
	padding-right: 18px;
	cursor: pointer;
}

#vietnam-view-detailed .overview-name .close-all {
	margin-left: 10px;
	background: url("../images/tour/icon/collapsed.png") no-repeat scroll right center transparent;
	color: #2A2A2A;
	font-weight: normal;
	padding-right: 18px;
	cursor: pointer;
}

#vietnam-view-detailed .overview-destmap {
	margin: 10px 0px;
	font-size: 14px;
	line-height: 20px;
}

#vietnam-view-detailed .overview-desc {
	margin: 10px 0px;
	font-size: 14px;
	font-style: italic;
	line-height: 20px;
}

#vietnam-view-detailed .overview-summary {
	background-color: #FFFFFF;
	border: 1px solid #DDDDDD;
	padding: 10px;
	margin: 10px 0px;
}

#vietnam-view-detailed .overview-summary ol,
#vietnam-view-detailed .overview-summary ul {
	list-style: inherit !important;
	margin-left: 40px !important;
}
#vietnam-view-detailed .overview-summary ol li,
#vietnam-view-detailed .overview-summary ul li {
	list-style: inherit !important;
}
#vietnam-view-detailed .overview-summary h2 * {
	font-size: 18px;
}
#vietnam-view-detailed hr {
	border: none;
	border-bottom: 1px solid #CC6633;
}

#vietnam-view-detailed .vietnam-view-itineraries {
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary {
	background: none repeat scroll 0 0 #FFFFFF;
	border: 1px solid #DDDDDD;
	margin-top: -1px;
	padding: 0px;
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .itinerary-header {
	padding: 10px;
	cursor: pointer;
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .itinerary-header:hover {
	background-color: #FFFFCC;
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .more-detail {
	display: none;
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .itinerary-title {
	text-transform: uppercase;
	font-size: 14px;
	font-weight: bold;
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .expanded a {
	background: url("../images/tour/icon/expanded.png") no-repeat scroll center center rgba(0, 0, 0, 0);
	cursor: pointer;
	width: 16px;
	height: 16px;
	display: inline-table;
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .collapsed a {
	background: url("../images/tour/icon/collapsed.png") no-repeat scroll center center rgba(0, 0, 0, 0);
	cursor: pointer;
	width: 16px;
	height: 16px;
	display: inline-table;
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .thumbnail {
