@CHARSET "ISO-8859-1";

body {
	background: none;
	background-color: #F0F0EF;
	color: #000;
}

#topmenu {
	right: 0px;
}

#topmenu ul li a {
	font-weight: normal;
}

#topmenu .phone {
	background-image: none;
	padding-left: 0px;
	font-weight: normal;
}
#mainmenu {
	margin: 0px;
	margin-top: -36px;
	padding: 0px;
	background-color: #FFFFFF;
	border-bottom: 4px solid #00539E;
}

#mainmenu .inner {
	position: relative;
}

#mainmenu .inner .submenu {
	margin: 0px;
	padding: 0px;
	background: none repeat scroll 0 0 #FFFFFF;
	box-shadow: 0 5px 5px rgba(0, 0, 0, 0.4);
	border-top: 4px solid #00539E;
	position: absolute;
	left: 0;
	width: 980px;
	z-index: 999;
	display: none;
}

#mainmenu .inner .handbook-submenu {
	margin: 0px;
	padding: 0px;
	background: url("../images/tour/menu/handbook-submenu-bg.jpg") no-repeat scroll right bottom #FFFFFF;
	box-shadow: 0 5px 5px rgba(0, 0, 0, 0.4);
	border-top: 4px solid #00539E;
	position: absolute;
	left: 0;
	width: 980px;
	height: 250px;
	z-index: 999;
	display: none;
}

#mainmenu #ex-service-submenu {
	margin: 0px;
	padding: 0px;
