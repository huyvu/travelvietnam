	border-top: 4px solid #00539E;
	position: absolute;
	left: 0;
	width: 980px;
	height: 250px;
	z-index: 999;
	display: none;
}

#mainmenu ul.megamenu > li .ex-service-submenu-content {
	padding: 10px;
}
