#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .itinerary-content .itinerary-detail {
}
#vietnam-view-detailed .vietnam-view-itineraries .vietnam-view-itinerary .itinerary-content .itinerary-detail p {
