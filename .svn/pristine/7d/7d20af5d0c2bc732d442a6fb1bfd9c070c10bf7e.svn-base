}
#mainmenu ul.megamenu > li .ex-service-submenu-content .ex-service img {
	width: 228px;
	height: 150px;
}

#megamenu {
	float: right;
}

#mainmenu ul.megamenu > li {
	border: none;
	position: inherit;
}

#mainmenu ul.megamenu > li > a {
