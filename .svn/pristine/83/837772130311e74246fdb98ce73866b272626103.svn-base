#vietnam-holiday .holiday-table thead {
	background-color: #B90B0B;
    border-bottom: 4px solid #FA514D;
    color: #FFFFFF;
    text-shadow: 1px 1px 1px #000000;
}

#vietnam-holiday .holiday-table thead tr th {
	background: none repeat scroll 0 center rgba(0, 0, 0, 0);
    color: #FFFFFF !important;
    padding: 5px 9px;
    text-align: left;
    vertical-align: top;
}

#vietnam-holiday .holiday-table tbody tr {
	background: none repeat scroll 0 0 #FFFFFF;
	border: 1px solid #DDDDDD;
}

#vietnam-holiday .holiday-table tbody tr:nth-child(2n) {
	background-color: #F7F7F7;
}

#vietnam-holiday .holiday-table tbody tr:hover {
	background: none repeat scroll 0 0 #DDDDDD;
}

#vietnam-holiday .holiday-table tbody tr td {
	padding: 5px 10px !important;
}

#year-select {
	text-align: center;
	margin-bottom: 30px;
	color: #176DB3;
}
