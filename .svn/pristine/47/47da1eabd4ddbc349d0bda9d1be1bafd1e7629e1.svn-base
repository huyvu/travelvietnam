/* Hotel View */
#hotel-view {
	margin: 10px 0px;
}
#hotel-view .hotelname {
	color: #535353;
    font-size: 24px;
    line-height: 24px;
}
#hotel-view .address {
    margin: 10px 0px 20px 0px;
}
#hotel-view .pricepanel {
	width: 250px;
    box-shadow: 2px 2px 2px 0 rgba(0, 0, 0, 0.2);
}
#hotel-view .pricepanel .content {
	border: 1px solid #13ABDC;
	padding: 10px;
}
#hotel-view .pricepanel .header {
	background-color: #13ABDC;
	line-height: 20px;
	padding: 8px 10px;
	color: #FFFFFF;
}
#hotel-view .pricepanel .header .saleprice {
	font-size: 12px;
	color: #FFFFFF;
	float: left;
}
#hotel-view .pricepanel .header .saleprice span {
	font-size: 20px;
	color: #FFFFFF;
	font-weight: bold;
}
#hotel-view .pricepanel .header .oldprice {
    color: #FFFFFF;
    float: right;
    font-size: 12px;
    text-decoration: line-through;
}
#hotel-view .pricepanel .values {
    margin: 10px 0;
}
#hotel-view .pricepanel .values div {
    text-align: center;
    width: 75px;
}
#hotel-view .pricepanel .values .label {
}
#hotel-view .pricepanel .values .value {
    font-size: 18px;
}
#hotel-view .pricepanel .separator {
    border-bottom: 1px solid #DDDDDD;
}
#hotel-view .pricepanel .requency {
	background: url("../images/availability.png") no-repeat scroll 0 0 transparent;
	margin: 10px;
	padding: 10px 10px 10px 35px;
}
#hotel-view .pricepanel .invite {
	background: url("../images/friend-invitation.png") no-repeat scroll 0 0 transparent;
	margin: 10px;
	padding: 6px 10px 10px 35px;
}
#hotel-view .pricepanel .share {
	background: url("../images/share-photo.png") no-repeat scroll 0 0 transparent;
	margin: 10px;
	padding: 10px 10px 10px 35px;
}
#hotel-view .pricepanel .rating {
	margin: 10px;
	padding: 10px;
}
#hotel-view .pricepanel .checkrate-button {
    text-align: center;
}
#hotel-view .pricepanel .checkrate-button a {
	background: url("../jquery/images/custom-slider/button-gradient.png") repeat-x
		scroll 0 0 #13ABDC;
	color: #FFFFFF;
	border-radius: 4px 4px 4px 4px;
	box-shadow: 0 1px 1px #999999;
	cursor: pointer;
	display: inline-block;
	font-size: 14px;
	font-weight: bold;
	padding: 8px 12px 8px;
}
#hotel-view .pricepanel .checkrate-button a:hover {
	background-image: none;
	text-decoration: none;
}
#hotel-view .amenities {
	width: 250px;
	margin-top: 25px;
}
#hotel-view .amenities .title {
	border-bottom: 1px solid #CCCCCC;
    border-top: medium none;
    margin: 0px 0px 20px;
    padding: 0px;
    position: relative;
}
#hotel-view .amenities .title .text {
	background-color: #FFFFFF;
    display: inline-block;
    padding: 0px 10px 0px 0px;
    position: relative;
    font-size: 16px;
    text-align: center;
    top: 9px;
}
#hotel-view .amenities ul {
	margin-left: 10px;
}
#hotel-view .amenities ul li {
	background: url("../images/amenities-check.gif") no-repeat scroll 0 0 transparent;
	text-indent: 20px;
	line-height: 18px;
	margin-top: 8px;
}
#hotel-view .b-images {
    border: 1px solid #CCCCCC;
    outline: medium none;
    overflow: hidden;
    padding: 2px;
    position: relative;
    height: 370px;
    width: 700px;
}
#hotel-view .b-images img {
	padding: 0px;
	margin: 0px;
    max-width: 700px;
}
#hotel-view .personalization-bar {
	background-color: #EEEEEE;
    border: medium none;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
    margin-bottom: 20px;
    width: 706px;
}
#hotel-view .personalization-bar-container {
	border-bottom: 2px solid #CCCCCC;
    padding: 5px 8px 4px;
    position: relative;
}
#hotel-view .personalization-bar-container .button {
    border: 1px solid #D3D3D3;
    background-color: #F8F8F8;
    background-image: linear-gradient(to bottom, #FCFCFC 0px, #F8F8F8 100%);
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
    color: #333333;
    cursor: pointer;
    display: inline-block;
    line-height: 20px;
    margin: 2px 4px 2px 0;
}
#hotel-view .personalization-bar-container .button:hover {
    background-image: linear-gradient(to bottom, #F8F8F8 0px, #EEEEEE 100%);
    border-color: #C6C6C6;
}
#hotel-view .personalization-bar-container .button a {
    color: #333333;
    text-decoration: none;
    line-height: 20px;
    padding: 2px 8px;
    display: inline-block;
}

#hotel-view-detail {
	margin-top: 10px;
	width: 690px;
	background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #DDDDDD;
    border-radius: 3px 3px 3px 3px;
    padding: 10px;
}
#hotel-view-detail ul.tabs {
	padding-bottom: 25px;
	border-bottom: 1px solid #D4D4D4;
}
#hotel-view-detail ul.tabs li {
	color: #666666;
    float: left;
    padding-right: 20px;
}
#hotel-view-detail ul.tabs li a.selected {
    color: #FF9C00;
    border-bottom: 3px solid #FF9C00;
    font-weight: bold;
}
#hotel-view-detail ul.tabs li a {
    color: #2A2A2A;
    font-size: 12px;
    padding: 8px 0px;
    text-decoration: none;
}
#hotel-view-detail div.tab-selected {
    display: block;
}
#hotel-view-detail div.tab-content {
    display: none;
}
#hotel-view-detail .content {
}
#hotel-view-detail .hotel-info-container {
    border-bottom: 1px solid #EFF1F1;
    margin: 10px 0;
    padding-bottom: 15px;
}
#hotel-view-detail .hotel-info-container .description {
    width: 520px;
    line-height: 18px;
    text-align: justify;
}
#hotel-view-detail .hotel-info-container .price {
    line-height: 26px;
    width: 140px;
}
#hotel-view-detail .hotel-info-container .price b {
    color: #555555;
}
#hotel-view-detail .hotel-info-container .price del {
    color: #555555;
    font-size: 14px;
}
#hotel-view-detail .hotel-info-container .price b {
    color: #FF9C00;
    font-size: 14px;
}
#hotel-view-detail .hotel-info-container .boxbook {
    display: inline;
    margin-top: 8px;
    width: 150px;
}
#hotel-view-detail .hotel-info-container .boxbook a.btnbook {
    background: url("../jquery/images/custom-slider/button-gradient.png") repeat-x scroll 0 0 #13ABDC;
    border-radius: 4px 4px 4px 4px;
    box-shadow: 0 1px 1px #999999;
    color: #FFFFFF;
    cursor: pointer;
    display: inline-block;
    float: right;
    font-size: 14px;
    font-weight: bold;
    padding: 8px 10px;
    text-align: center;
    text-decoration: none;
    width: 120px;
}
#hotel-view-detail .hotel-info-container .boxbook a.btnbook:hover {
    background-image: none;
    text-decoration: none;
}
#hotel-view-detail .hotel-info-container .boxbook a.btncustomize {
    background: url("../jquery/images/custom-slider/button-gradient.png") repeat-x scroll 0 0 #13ABDC;
    border-radius: 4px 4px 4px 4px;
    box-shadow: 0 1px 1px #999999;
    color: #FFFFFF;
    cursor: pointer;
    display: inline-block;
    float: right;
    font-size: 14px;
    font-weight: bold;
    margin-top: 8px;
    padding: 8px 10px;
    text-align: center;
    text-decoration: none;
    width: 120px;
}
#hotel-view-detail .hotel-info-container .boxbook a.btncustomize:hover {
    background-image: none;
    text-decoration: none;
}
.box-highlight {
    border-bottom: 1px solid #EFF1F1;
    margin: 10px 0;
    padding-bottom: 15px;
}
.top-highlight {
    font-size: 16px;
    font-weight: bold;
    padding-bottom: 10px;
}
.info-highlight {
}
.info-highlight p {
    line-height: 20px;
    text-align: justify;
