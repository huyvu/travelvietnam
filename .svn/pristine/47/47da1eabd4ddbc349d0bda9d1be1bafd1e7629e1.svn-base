/* Recommendation */
.recommendation {
}
.recommendation h1 {
	background-color: #FF8800;
    padding: 2px 10px;
    font-family: Times New Roman;
    font-weight: bold;
    line-height: 26px;
    color: #FFFFFF;
}
.recommendation ul {
	margin: 8px 0px;
}
.recommendation ul li {
	float: left;
	width: 170px;
	margin-left: 10px;
	margin-bottom: 10px;
}
.recommendation ul li.first {
	margin-left: 0px;
}
.recommendation ul li.last {
	float: right;
}
.recommendation ul li .name {
	font-weight: bold;
	line-height: 24px;
}
