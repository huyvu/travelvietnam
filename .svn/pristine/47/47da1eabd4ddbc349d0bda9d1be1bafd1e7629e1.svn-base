@CHARSET "ISO-8859-1";

/* Template Layout */

body {
	background: url("../images/hotel-background.jpg") no-repeat scroll top center transparent;
	color: #2A2A2A;
}

#home-wizban {
	height: 310px;
	position: relative;
}

.travelads {
	margin: 0;
	height: 310px;
	background: url(../jquery/images/assets/hotel-home.jpg) no-repeat scroll 0 0 transparent;
	position: relative;
	overflow: hidden;
}

#mainmenu {
	background-color: #A2652C;
}

#mainmenu ul.megamenu > li {
    border-left: 1px solid #AA6E3A;
    border-right: 1px solid #A2652C;
}

#mainmenu ul.megamenu > li > a {
	background-color: #A2652C;
}

#mainmenu ul.megamenu > li > a:hover, #mainmenu ul.megamenu > li.selected > a {
	background-color: #A2652C;
	color: #FFF3B0;
}

#topmenu {
    right: 0px;
}

#topmenu ul li a {
}

/* End Template Layout */

/* Custom Controls */

#hotel-search-widget {
	background: url("../images/hotel-search-widget-bg.png") no-repeat scroll 0 0 transparent;
	width: 248px;
	height: 528px;
	margin: 0px;
	position: absolute;
	display: block;
	left: 10px;
	top: 10px;
	z-index: 900;
}

#hotel-search-widget .widget-content {
	clear: both;
	margin-top: 235px;
	padding: 10px 5px;
}

#hotel-search-widget .widget-content p.text-form {
    display: inline-block;
    float: left;
    margin: 10px 0px 0px 10px;
}

#hotel-search-widget .widget-content p.w180 {
	width: 100px;
}

#hotel-search-widget .widget-content .text-form label {
	display: block;
	margin-bottom: 4px;
}

#hotel-search-widget .widget-content .text-form span {
	background: url("../images/calendar-icon.png") no-repeat scroll 85px 4px #FEFEFE;
	border: 1px solid #AAAAAA;
	float: left;
	padding: 2px 6px 4px 6px;
	width: 90px;
}

#hotel-search-widget .widget-content .text-form input.text-input {
	width: 200px;
	border: 1px solid #AAAAAA;
	padding: 2px 6px 4px 6px;
}

#hotel-search-widget .widget-content .text-form select {
	width: 172px;
	border: 1px solid #AAAAAA;
	padding: 2px 4px;
}

#hotel-search-widget .widget-content .text-form span input.calendar {
	background: none repeat scroll 0 0 transparent;
	border: medium none;
	float: none;
	width: 75px;
	font-size: 12px;
}

#hotel-search-widget .widget-content .text-form .map-ticker {
	border: 0px;
	bottom: 20px;
	right: 120px;
	position: absolute;
	cursor: pointer;
}

#hotel-search-widget .widget-content .text-form .search-button {
	bottom: 20px;
	right: 15px;
	position: absolute;
	cursor: pointer;
}

#hotel-search-widget .widget-content .text-form .findhotel-button {
	background: url("../images/btn-find-hotel.png") no-repeat scroll 0 0 transparent;
	width: 114px;
	height: 41px;
}

#hotel-search-widget .widget-content .text-form .findtour-button {
	background: url("../images/btn-find-hotel.png") no-repeat scroll 0 0 transparent;
	width: 114px;
	height: 41px;
}

/* End Custom Controls */

/* Best Selling */
.hotels {
    float: left;
    margin-top: 15px;
    width: 100%;
}
.hotels-l {
    float: left;
    width: 650px;
}
.besttabs {
    border-bottom: 1px solid #FF8A00;
    float: left;
    height: 30px;
    position: relative;
    width: 100%;
}
.besttabs h2 {
    color: #DD7734;
    float: left;
    font-size: 14px;
    font-weight: bold;
    padding-top: 10px;
    text-transform: uppercase;
}
ul#citytab {
    float: right;
    position: relative;
}
ul#citytab li {
    background: none repeat scroll 0 0 #ECECEC;
    display: inline;
    float: left;
    font-size: 12px;
    font-weight: bold;
    height: 30px;
    line-height: 30px;
    margin-left: 3px;
    position: relative;
    text-transform: uppercase;
}
ul#citytab li a {
    color: #5E5D5D;
    padding: 0 14px;
    text-decoration: none;
}
ul#citytab li a:hover {
    color: #4A4A4A;
    text-decoration: none;
}
ul#citytab li.current {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: url("../images/1pxhotels.gif") repeat-x scroll 0 -565px transparent;
    border-color: #C2C2C2 #C2C2C2 -moz-use-text-color;
    border-image: none;
    border-style: solid solid none;
    border-width: 1px 1px 0;
    height: 30px;
}
ul#citytab li.current a {
    color: #DD7734;
}
#morehotels {
    cursor: pointer;
    float: left;
    position: relative;
}
#listhotels {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #ECECEC;
    border-color: -moz-use-text-color #CECECE #CECECE;
    border-image: none;
    border-right: 1px solid #CECECE;
    border-style: none solid solid;
    border-width: 0 1px 1px;
    display: none;
    float: left;
    padding: 10px;
    position: absolute;
    right: 0;
    top: 31px;
    width: 150px;
    z-index: 99999;
}
#listhotels ul {
    margin: 0;
    width: 100%;
}
#listhotels ul li {
    border-bottom: 1px solid #F4F4F4;
    color: #555555;
    float: left;
    margin: 0;
    padding: 0;
    width: 100%;
}
#listhotels ul li.last {
    border-bottom: none;
}
#listhotels ul li a {
    color: #5E5D5D;
    padding: 0;
    text-decoration: none;
}
#listhotels ul li a:hover {
    text-decoration: underline;
}
ul.listhotels {
    float: left;
    width: 100%;
}
ul.listhotels li {
    float: left;
    margin-top: 15px;
    width: 310px;
}
ul.listhotels li img.thumb {
    border: 1px solid #EAEAEA;
    display: inline;
    float: left;
    margin-right: 18px;
    padding: 2px;
    width: 100px;
}
ul.listhotels li span.info {
    float: left;
    height: 100px;
    width: 165px;
}
ul.listhotels li span.info h2 {
    font-size: 12px;
    font-weight: bold;
    width: 100%;
}
ul.listhotels li span.info p {
    color: #888888;
    font-size: 11px;
    padding: 2px 0 8px;
    width: 100%;
}
ul.listhotels li span.info font.price {
    color: #444444;
    font-size: 12px;
}
ul.listhotels li span h2 a {
    color: #DD7938;
    text-decoration: none;
}
.starhotel {
    background: url("../images/star.gif") no-repeat scroll 0 0 transparent;
    height: 12px;
    padding-right: 0;
    width: 64px;
}
img.star1 {
    background-position: 0 0;
}
img.star2 {
    background-position: 0 -12px;
}
img.star3 {
    background-position: 0 -24px;
}
img.star4 {
    background-position: 0 -36px;
}
img.star5 {
    background-position: 0 -48px;
}
.hotels-r {
    float: right;
    position: relative;
    width: 300px;
}
.ttpromotion {
    background-color: #70A305;
    color: #FFFFFF;
    float: left;
    font-size: 14px;
    font-weight: bold;
    height: 32px;
    line-height: 32px;
    position: relative;
    text-align: center;
    text-transform: uppercase;
    width: 100%;
}
.arrowprm {
    background-position: 0 -200px;
    height: 7px;
    left: 48%;
    position: absolute;
    top: 32px;
    width: 12px;
    z-index: 2;
}
ul.listprm {
    background: none repeat scroll 0 0 #FBFBFB;
    float: left;
    padding-bottom: 15px;
    position: relative;
    width: 300px;
    z-index: 1;
}
ul.listprm li {
    background: url("../images/line.gif") no-repeat scroll center bottom transparent;
    float: left;
    margin-top: 15px;
    padding-bottom: 12px;
    text-align: center;
    width: 100%;
}
ul.listprm li a {
    color: #DD7938;
    text-decoration: none;
}
ul.listprm li p {
    text-align: center;
}
ul.listprm li h2 {
    font-size: 12px;
    font-weight: bold;
    text-align: center;
}
ul.listprm li p {
    color: #888888;
    font-size: 11px;
    padding-top: 8px;
    width: 100%;
}
ul.listprm li font.num {
    color: #70A305;
    font-size: 12px;
    font-weight: bold;
}
ul.listprm li.end {
    background: none repeat scroll 0 0 transparent;
    padding-bottom: 0;
}
ul.listprm li a.view {
    color: #5C5C5C;
    font-size: 12px;
    padding: 0 3px;
    text-decoration: none;
}
ul.listprm li a.view:hover {
    text-decoration: underline;
}
/* End Best Selling */

